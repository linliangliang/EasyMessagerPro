<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    
    <TextView 
        android:id="@+id/worklist_scan_send1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="20dp"
        android:gravity="center"/>
	<TextView 
        android:id="@+id/worklist_scan_send2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/worklist_scan_send1"
        android:layout_marginRight="20dp"
        android:gravity="center"/>

	<TextView
		android:id="@+id/worklist_scan_send3"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginRight="20dp"
		android:layout_toRightOf="@id/worklist_scan_send2"
		android:gravity="center" />

	<TextView
        android:id="@+id/worklist_scan_send4"
		android:layout_width="wrap_content"
        android:layout_height="wrap_content"
		android:layout_toRightOf="@id/worklist_scan_send3"
        android:layout_marginRight="20dp"
        android:gravity="center"/>
	<TextView 
        android:id="@+id/worklist_scan_send5"
       	android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/worklist_scan_send4"
        android:gravity="center"/>
</RelativeLayout>
